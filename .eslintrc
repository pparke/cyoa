{
    "extends": "airbnb-base",
    "env": {
        "browser": true,
        "node": true,
    },
    "rules": {
        "indent": ["error", "tab"],
        "no-tabs": 0,
        "no-multi-assign": 0,
        "comma-dangle": 0,
        "no-use-before-define": 0,
        "no-console": 0,
        "brace-style": ["error", "stroustrup"],
        "prefer-destructuring": 0,
        "no-param-reassign": 0,
        "no-underscore-dangle": 0,
        "func-names": 0,
        "prefer-arrow-callback": 0,
        "no-mixed-operators": 0,
        "class-methods-use-this": 0,
        "wrap-iife": 0,
        "quotes": 0
    }
}
